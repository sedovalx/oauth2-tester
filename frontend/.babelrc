{
  "presets": ["react", "es2015", "stage-0"],
  "plugins": [
    "transform-object-assign",
    ["babel-plugin-module-alias", [
      { "src": "./src/actions", "expose": "actions" },
      { "src": "./src/config", "expose": "config" },
      { "src": "./src/rest", "expose": "rest" },
      { "src": "./src/router", "expose": "router" },
      { "src": "./src/reducers", "expose": "reducers" },
      { "src": "./src/ui/component", "expose": "component" },
      { "src": "./src/ui/container", "expose": "container" },
      { "src": "./assets/styles", "expose": "styles" },
      { "src": "./assets/lang", "expose": "lang" },
      { "src": "./src/props", "expose": "props" },
      { "src": "./src/validation", "expose": "validation" },
      { "src": "./src/services", "expose": "services" }
    ]]
  ],
  "env": {
    "development": {
      "presets": ["react-hmre"]
    }
  }
}